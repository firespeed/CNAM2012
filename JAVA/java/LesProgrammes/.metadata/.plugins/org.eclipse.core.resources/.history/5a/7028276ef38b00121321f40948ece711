
class Couple<T>
{ 
	private T x, y ; // les deux elements du couple
	
	public Couple (T premier, T second)
	{ 
		x = premier;
		y = second ;
	}

	public T getPremier(){ return x ; }

	public void affiche ()
	{ 
		System.out.println ("premiere valeur : " + x + " - deuxieme valeur : " + y ) ;
	}
}
